{
  "name": "dioptase-assembly",
  "displayName": "Dioptase Assembly",
  "description": "Syntax highlighting and language support for the Dioptase assembly language.",
  "publisher": "b-Rocks2718",
  "repository": {
    "type": "git",
    "url": "https://github.com/b-Rocks2718/Dioptase-Assembler.git"
  },
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dioptase",
        "aliases": [
          "Dioptase Assembly",
          "dioptase"
        ],
        "extensions": [
          ".s",
          ".asm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dioptase",
        "scopeName": "source.dioptase",
        "path": "./syntaxes/dioptase.tmLanguage.json"
      }
    ]
  },
  "overrides": {
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@vscode/vsce": "^2.30.0"
  }
}
